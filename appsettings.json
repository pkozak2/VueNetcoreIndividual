{
  "ConnectionStrings": {
    "DefaultConnection": "DataSource=app.db"
  },
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "IdentityServer": {
    "Clients": {
      "auth": {
        "Profile": "IdentityServerSPA",
        "RedirectUri": "/authentication/login/login-callback",
        "LogoutUri": "/authentication/logout/logout-callback"
      }
    }
  },
  "AllowedHosts": "*"
}
